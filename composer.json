{
    "name": "chemem/bingo-router",
    "description": "A simple router for the Bingo Framework.",
    "type": "library",
    "license": "Apache",
    "authors": [
        {
            "name": "Lochemem Bruno Michael",
            "email": "lochbm@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Chemem\\Bingo\\Router\\": "src/",
            "Chemem\\Bingo\\Router\\Helpers\\": "src/Helpers"
        },
        "files": [
            "src/Helpers/Identity.php",
            "src/Helpers/Compose.php"
        ]
    },
    "suggest": {
        "chemem/bingo-conveyor": "A simple HTML view conveyor for the Bingo Framework",
        "mustache/mustache": "A simple logic-less template engine",
        "pug-php/pug": "HAML-based template engine for PHP"
    },
    "config": {
        "sort-packages": "true",
        "preferred-install": "dist"
    }
}
